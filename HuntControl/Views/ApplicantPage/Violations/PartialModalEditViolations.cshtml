<script>
    $('[data-parent-input]').change(function () {
        var parent_el = $(this);
        $('[data-child_input]').each(function () {
            if ($(this).data('child_input') == parent_el.attr("name")) {
                if (parent_el.hasClass("select2")) {
                    if (parent_el.children("option:selected").val() === undefined || parent_el.children("option:selected").val() == "") {
                        $(this).val("");
                    }
                    else {
                        $(this).val(parent_el.children("option:selected").text());
                    }
                }
                else {
                    $(this).val(parent_el.val());
                }
            }
        }
        );
    });
    $('#pr_date_create').change(function () {
        $('[data-child_input=pr_date_create]').each(function () {
            $(this).val($('#pr_date_create').val() + " " + $('#pr_time_create').val());
        });
    });
    $('#pr_time_create').change(function () {
        $('[data-child_input=pr_date_create]').each(function () {
            $(this).val($('#pr_date_create').val() + " " + $('#pr_time_create').val());
        });
    });

    $('#btn_edit_violation').click(function () {
        ClearDateHideTab();
        var $form = $("#EditViolationsForm");
        var $data = null;
        $data = $form.serialize();
            $.ajax({
                url: '@Url.Action("SubmitViolationsSave", "ApplicantPage")',
                type: 'POST',
                data: $data,
                beforeSend: function () {
                    $('#myModal').block({ message: '<div class="fa fa-spin fa-spinner icon-lg"></div>', overlayCSS: { backgroundColor: "#FFF", cursor: "wait" }, css: { border: 0, padding: 30, backgroundColor: "none" } });
                },
                success: function (data) {
                    successSave();
                    $('#customerViolationsTabContainer').html(data);
                    $('#myModal').unblock();
                    $('#myModal').modal('hide');
                }
            });
    });

    function ClearDateHideTab() {
        $('a[data-form-name]').each(function () {
            var tab = $(this);
            var tabli = $(this).closest('li');
            if (tabli.hasClass('hidden')) {
                $("[data-form]").each(function () {
                    if ($(this).data("form") == tab.data("form-name")) {
                        $(this).val('');
                    }
                });
            }
        });
        $('div[data-form-name]').each(function () {
            var tab = $(this);
            if (tab.hasClass('hidden')) {
                $("[data-form]").each(function () {
                    if ($(this).data("form") == tab.data("form-name")) {
                        $(this).val('');
                    }
                });
            }
        });
    }
</script>