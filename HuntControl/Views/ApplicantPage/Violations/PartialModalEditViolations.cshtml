<script>
    $(document).ready(function () {
        $('.selectpicker').selectpicker();
        $('.select2').select2({
            language: "ru"
        });

        var elem = document.querySelector('#res_payment');
        var init = new Switchery(elem);

                $('input[data-type=dateTime]').datetimepicker({
            singleDatePicker: true,
            timePicker: true,
            timePicker24Hour: true,
            autoclose: true,
            format: "dd.mm.yyyy hh:ii",
            orientation: 'left',
           
                });

        $('input[data-type=date]').datepicker({ format: 'dd.mm.yyyy', autoclose: true, language: 'ru', orientation: 'left' });
        $('input[data-type=time]').clockpicker({ language: 'ru', donetext: 'Выбрать', format: 'hh:mm' });

        if ('@Model.pr_date_law_violation.ToString()' == "") { $("#pr_date_law_violation").val('').trigger('change'); }
        if ('@Model.pr_date_examination.ToString()' == "") { $("#pr_date_examination").val('').trigger('change'); }
        if ('@Model.pl_date.ToString()' == "") { $("#pl_date").val('').trigger('change'); }

        function definitionTypeField(typeId) {
            switch (typeId) {
                case '1':
                    $('a[href=#decreeDataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#fsspDataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#disDataTabContainer]').closest('li').addClass('hidden');

                    $('#plDataContainer').removeClass('hidden');
                    break;
                case '2':
                    $('a[href=#decreeDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#fsspDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').removeClass('hidden');
                    $('a[href=#disDataTabContainer]').closest('li').addClass('hidden');

                    $('#plDataContainer').addClass('hidden');
                    break;
                case '3':
                    $('a[href=#decreeDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#fsspDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#disDataTabContainer]').closest('li').removeClass('hidden');

                    $('#plDataContainer').addClass('hidden');
                    break;
               case '':
                    $('a[href=#decreeDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#fsspDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#disDataTabContainer]').closest('li').addClass('hidden');

                    $('#plDataContainer').addClass('hidden');
                    break;
                default:
            }

        }

        definitionTypeField($('#def_spr_definition_type_id').val());

        $('#def_spr_definition_type_id').change(function () {
            definitionTypeField($(this).val());
        });
        $('#res_payment').change(function () {
            if (!elem.checked && $('#def_spr_definition_type_id').val() == 1) {
                $('a[href=#fsspDataTabContainer]').closest('li').removeClass('hidden');
                $('a[href=#pr20NotifDataTabContainer]').closest('li').removeClass('hidden');
                $('a[href=#pr20DataTabContainer]').closest('li').removeClass('hidden');
                $('a[href=#courtDataTabContainer]').closest('li').removeClass('hidden');
            } else {
                if ($('#def_spr_definition_type_id').val() == 2) {
                    $('a[href=#fsspDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').removeClass('hidden');
                }
                else {
                    $('a[href=#fsspDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20NotifDataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#pr20DataTabContainer]').closest('li').addClass('hidden');
                    $('a[href=#courtDataTabContainer]').closest('li').addClass('hidden');
                }
            }
        });
        $('#res_payment').change();

        $('#spr_violations').change(function () {
        var selected = $(this).val() != null && $(this).val() !== '';
        if (selected) {
            $.ajax({
                url: '@Url.Action("GetSprViolationsPart", "ApplicantPage")',
                data: { violations_id: $('#spr_violations').val() },
                success: function (data) {
                    $("#pr_spr_violations_part_id").html(null).select2({
                        data: data,
                    }).prop("disabled", false).val('').trigger('change');
                }
            });
        } else {
            $('#pr_spr_violations_part_id').prop("disabled", true).val('').trigger('change');
        }
        });

        $('#bai_spr_bailiffs_id').change(function () {
        var selected = $(this).val() != null && $(this).val() !== '';
        if (selected) {
            $.ajax({
                url: '@Url.Action("GetSprBailiffs", "ApplicantPage")',
                data: { bailiffs_id: $('#bai_spr_bailiffs_id').val() },
                success: function (data) {
                    $('#bai_name_bailiffs').val(data.name_bailiffs);
                    $('#bai_head').val(data.head_bailiffs);
                    $('#bai_address').val(data.address_bailiffs);
                }
            });
        }
        else {
            $('#bai_name_bailiffs').val("");
            $('#bai_head').val("");
            $('#bai_address').val("");
        }
        });
        $('#tr_spr_organization_id').change(function () {
        var selected = $(this).val() != null && $(this).val() !== '';
        if (selected) {
            $.ajax({
                url: '@Url.Action("GetSprOrganizations", "ApplicantPage")',
                data: { organization_id: $('#tr_spr_organization_id').val() },
                success: function (data) {
                    $('#tr_name_organization').val(data.name_organization);
                }
            });
        } else {
            $('#tr_name_organization').val("");
        }
        });

        $('#pr_spr_employees_id_create').change(function () {
        var selected = $(this).val() != null && $(this).val() !== '';
        if (selected) {
            $.ajax({
                url: '@Url.Action("GetSprEmployees", "ApplicantPage")',
                data: { employees_id: $('#pr_spr_employees_id_create').val() },
                success: function (data) {
                    console.log(data);
                    $('#pr_name_create').val(data.employees_fio);
                    $('#pr_job_pos_name').val(data.job_pos_name);
                }
            });
        }
        else {
            $('#pr_name_create').val("");
            $('#pr_job_pos_name').val("");
        }
        });

        $('[data-parent-input]').each(function () {
            var parent_el = $(this);
            $('[data-child_input]').each(function () {
                if ($(this).data('child_input') == parent_el.attr("name")) {
                    if (parent_el.hasClass("select2")) {
                        if (parent_el.children("option:selected").val() === undefined || parent_el.children("option:selected").val() == "") {
                            $(this).val("");
                        }
                        else {
                            $(this).val(parent_el.children("option:selected").text());
                        }
                    }
                    else {
                        if (parent_el.attr("name") == "pr_date_create") {
                            $(this).val(parent_el.val() + " " + $("#pr_time_create").val());
                        }
                        else { $(this).val(parent_el.val()); }
                    }
                }
            }
            );
        });
        $("#tr_spr_organization_result_id").change(function () {
            if ($(this).val() == 2) { $("#div_tr_payment").removeClass("hidden"); }
            else { $("#div_tr_payment").addClass("hidden"); }
        });
        $("#tr_spr_organization_result_id").change();

        $('[data-relation-input]').each(function () {
            var $this = $(this);
            var inputs = $(this).data('relationInput').split(',');
            var check = false;
            $.each(inputs, function (index, value) {
                var input = $('#' + value);
                if (input.val() != "") {
                    check = true;
                }
            });
            if (!check) { $this.find("i").removeClass("fa-check green").addClass("fa-times red"); }
            else { $this.find("i").removeClass("fa-times red").addClass("fa-check green");}
        });
    });
</script>
