<style>
    input:disabled, textarea:disabled, input:read-only, textarea:read-only {
        background-color: #efefef !important;
    }

    .panel-title.font-14 {
        margin-left: -10px;
    }

    .red {
        color: red !important;
    }

    .green {
        color: green !important;
