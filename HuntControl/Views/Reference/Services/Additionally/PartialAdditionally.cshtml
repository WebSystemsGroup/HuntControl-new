<div class="tab-content br-n pn">
    <div id="customersPillContainer" class="tab-pane active">
        @Html.Action("PartialTableSubServiceCustomers", "Reference", new { subServiceId = ViewBag.SubServiceId })
    </div>
    <div id="documentsPillContainer" class="tab-pane active">
    </div>
    <div id="SmevPillContainer" class="tab-pane">
    </div>
    <div id="resultDocsPillContainer" class="tab-pane">
    </div>
    <div id="failuresPillContainer" class="tab-pane">
    </div>
    <div id="failureDocsPillContainer" class="tab-pane">
    </div>
    <div id="wayGetsPillContainer" class="tab-pane">
    </div>
    <div id="wayGetResultsPillContainer" class="tab-pane">
    </div>
    <div id="activityPillContainer" class="tab-pane">
    </div>
    <div id="filesPillContainer" class="tab-pane">
    </div>
</div>

<script>
    $('a[href="#customersPillContainer"]').click(function () {
        $.ajax({
            type: 'POST',
            url: 'PartialTableSubServiceCustomers',
            data: { subServiceId: '@ViewBag.SubServiceId' },
            success: function (data) {
                $('#customersPillContainer').html(data);
            }
        });
    });

    $('a[href="#documentsPillContainer"]').click(function () {
        $.ajax({
            type: 'POST',
