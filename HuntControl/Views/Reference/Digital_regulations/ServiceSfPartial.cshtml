@model WebAIS.Models.KcrModel.serviceSFType

<div class="row justify-content-end">
    <div class="col-lg-11 accardion-milti-castoner">
        <div class="accardion-milti-castoner-сontrol">
            <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                Сервис СМЭВ
            </button>
            <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
        </div>
        <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
            <fieldset>
                <div class="row justify-content-center">
                    <table class="table">
                        <tbody>
                            @if (Model.id != null)
                            {
                                <tr>
                                    <td>Идентификатор</td>
                                    <td>@Model.id</td>
                                </tr>
                            }
                            @if (Model.name != null)
                            {
                                <tr>
                                    <td>Наименование сервиса</td>
                                    <td>@Model.name</td>
                                </tr>
                            }
                            @if (Model.idService != null)
                            {
                                <tr>
                                    <td>ID сервиса</td>
                                    <td>@Model.idService</td>
                                </tr>
                            }
                            @if (Model.ownerService != null)
                            {
                                <tr>
                                    <td>Владелец сервиса</td>
                                    <td>@Model.ownerService.id</td>
                                </tr>
                            }
                            @if (Model.versionService != null)
                            {
                                <tr>
                                    <td>Версия сервиса</td>
                                    <td>@Model.versionService</td>
                                </tr>
                            }
                            @if (Model.linkService != null)
                            {
                                <tr>
                                    <td>Ссылка</td>
                                    <td>@Model.linkService</td>
                                </tr>
                            }
                            @if (Model.dictionaryCode != null)
                            {
                                <tr>
                                    <td>Код справочника</td>
                                    <td>@Model.dictionaryCode</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </fieldset>
        </div>
    </div>
</div>