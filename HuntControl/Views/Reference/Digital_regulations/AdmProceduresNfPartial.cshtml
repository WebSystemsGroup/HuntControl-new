@model IEnumerable<WebAIS.Models.KcrModel.admProceduresNFType>

<div class="row justify-content-end">
    <div class="col-lg-11 accardion-milti-castoner">
        <div class="accardion-milti-castoner-сontrol">
            <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                Административные процедуры
            </button>
            <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
        </div>
        <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
            <fieldset>
                @foreach (var procedure in Model)
                {
                    <div class="row justify-content-end">
                        <div class="col-lg-11 accardion-milti-castoner">
                            <div class="accardion-milti-castoner-сontrol">
                                <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                    Административная процедура
                                </button>
                                <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                            </div>
                            <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                <fieldset>
                                    <div class="row justify-content-center">
                                        <table class="table">
                                            <tbody>
                                                @if (procedure.admProcedure != null)
                                                {
                                                    <tr>
                                                        <td>Наименование</td>
                                                        <td>@procedure.admProcedure.name</td>
                                                    </tr>
                                                }
                                                @if (procedure.deadline != null)
                                                {
                                                    <tr>
                                                        <td>Срок выполнения</td>
                                                        <td>@procedure.deadline @(procedure.unitsOfMeasurement != null ? procedure.unitsOfMeasurement.name : "")</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                }
            </fieldset>
        </div>
    </div>
</div>