@model IEnumerable<WebAIS.Models.KcrModel.digitalRegulations>
@{
    ViewBag.Title = "Справочник цифровых регламентов";
}
<link href="~/Content/libs/jstree/style.min.css" rel="stylesheet" />
<style>
    .vakata-context, .vakata-context ul {
        z-index: 9999;
    }
    /*temp style*/

    td {
        vertical-align: top;
    }

    #nameList td {
        cursor: pointer
    }

        #nameList td.active {
            background-color: aliceblue !important;
        }

    .accardion-milti-castoner {
        padding: 0px;
        border-radius: 4px;
    }

    .row {
        display: -ms-flexbox;
        display: flex;
        -ms-flex-wrap: wrap;
        flex-wrap: wrap;
        margin-right: -15px;
        margin-left: -15px;
    }

    .col-8 {
        -ms-flex: 0 0 66.666667%;
        flex: 0 0 66.666667%;
        max-width: 66.666667%;
    }

    .col-4 {
        -ms-flex: 0 0 33.333333%;
        flex: 0 0 33.333333%;
        max-width: 33.333333%;
    }

    .col-4, .col-8 {
        position: relative;
        width: 100%;
        min-height: 1px;
        padding-right: 15px;
        padding-left: 15px;
    }

    .table {
        width: 100%;
        max-width: 100%;
        margin-bottom: 1rem;
        background-color: transparent;
    }

    .col-lg-11 {
        -ms-flex: 0 0 91.666667%;
        flex: 0 0 91.666667%;
        max-width: 91.666667%;
    }

    .justify-content-center {
        -ms-flex-pack: center !important;
        justify-content: center !important;
    }

    .accardion-milti-castoner-сontrol {
        display: flex;
        align-items: center;
        position: relative;
        margin-bottom: 1rem;
    }

    .accardion-milti-castoner-btn-show {
        width: 100%;
        border-right: 0px;
        box-shadow: none;
        background-color: #f9f7f7;
        border-radius: 3px 0px 0px 3px;
        min-height: 46px !important;
    }

    .accardion-milti-castoner-btn-remove i {
        color: #e04e39;
    }

    .accardion-chevron {
        position: absolute;
        top: 16px;
        left: 10px;
    }

    .accardion-content-milti-castoner {
        padding: 0px 15px;
    }

    .table .table {
        background-color: #fff;
        border: 1px solid grey;
    }
</style>
<div class="content" id="reference-services-main-content">
    <div class="container-fluid">
        @Html.MvcSiteMap().SiteMapPath()
        <div class="row">
            <div class="col-xl-10">
                <div class="card-box">
                    <div class="row">
                        <div class="col-4">
                            <form class="form-horizontal" id="cr">
                                <table class="table" id="nameList">
                                    <thead>
                                        <tr>
                                            @*<th style="text-align:center">Идентификатор</th>*@
                                            <th style="text-align:center">Наименование регламента</th>
                                            @*<th style="text-align:center">Наименование услуги</th>*@
                                        </tr>
                                    </thead>
                                    <tbody id="crList">
                                        @foreach (var item1 in Model)
                                        {
                                            var kcr = item1.drData;
                                            <tr>
                                                <td style="text-align:center; display: none"><input type="text" name="crId" value="@kcr.secondaryFields.overview.id"></td>
                                                <td data-id="@kcr.secondaryFields.overview.id">@kcr.normativeFields.overview.servicefullName</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </form>
                            <button type="button" onclick="addNew()" class="btn btn-inverse waves-effect waves-light">Добавить<i class="fa fa-plus m-l-5"></i></button>
                            <button type="button" onclick="save()" class="btn btn-success waves-effect waves-light">Сохранить<i class="fa fa-check m-l-5"></i></button>

                        </div>

                        <div class="col-8">
                            <table class="table" id="contentList">
                                <thead>
                                    <tr>
                                        <th style="text-align:center">Информация</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            @foreach (var itemCr in Model)
                                            {
                                                var kcr = itemCr.drData;
                                                <div id="@kcr.secondaryFields.overview.id" data-cr-content>
                                                    <div class="row mb-10">
                                                        <div class="col-lg-6 accardion-milti-castoner" style="margin-bottom: 10px;">
                                                            <button type="button" onclick="update(this)" class="btn waves-effect waves-light">Обновить регламент<i class="fa fa-refresh m-l-5"></i></button>
                                                        </div>
                                                    </div>

                                                    @if (!string.IsNullOrWhiteSpace(itemCr.serviceData?.service_name))
                                                    {
                                                        <div class="row mb-12 justify-content-center" data-service>
                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                <div>
                                                                    <div class="col-lg-9 form-group" style="margin-bottom: 10px">
                                                                        <textarea class="form-control" style="width:100%; resize: vertical" placeholder="Наименование услуги" id="serviceName" name="serviceName">@kcr.normativeFields.overview.servicefullName</textarea>
                                                                        <textarea class="form-control" style="display: none" name="serviceId">@itemCr.serviceData.id</textarea>
                                                                        <label class="custom-label" for="serviceName">Наименование услуги</label>
                                                                    </div>
                                                                    <div class="col-lg-2" style="margin-bottom: 10px">
                                                                        <button type="button" onclick="updateServicaName(this)" class="btn waves-effect btn-success waves-light">Сохранить<i class="fa fa-check m-l-5"></i></button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }

                                                    @if (itemCr.tariff?.Length > 0)
                                                    {
                                                        <div class="row mb-12 justify-content-center" data-tariff>
                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                <div>
                                                                    <div class="col-lg-6 form-group" style="margin-bottom: 10px">
                                                                        <div class="custom-field">
                                                                            <input type="number" min="0" max="30" maxlength="2" class="custom-input form-control custom-input-value" id="countDay" name="countDay" value="@(kcr.normativeFields.subServices[0]?.periodRegistration?.maxPeriodOfProviding)">
                                                                            <input type="text" style="display: none" class="custom-input form-control custom-input-value" name="tariffId" value="@itemCr.tariff[0].id">
                                                                            <label class="custom-label" for="countDay">Срок исполнения</label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col" style="margin-bottom: 10px">
                                                                        <button type="button" onclick="updateTariff(this)" class="btn waves-effect btn-success waves-light">Сохранить<i class="fa fa-check m-l-5"></i></button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                    <div class="row justify-content-center">
                                                        <div class="col-lg-11 accardion-milti-castoner">
                                                            <div class="accardion-milti-castoner-сontrol">
                                                                <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                    Нормативные поля
                                                                </button>
                                                                <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                            </div>

                                                            <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                <fieldset>
                                                                    <div class="row justify-content-end">
                                                                        <table class="table">
                                                                            <tbody>
                                                                                @if (kcr.normativeFields.overview.hasExtraterritorialitySpecified)
                                                                                {
                                                                                    <tr>
                                                                                        <td>Услуга может предоставляться экстерриториально?</td>
                                                                                        <td>@(kcr.normativeFields.overview.hasExtraterritoriality ? "да" : "нет")</td>
                                                                                    </tr>
                                                                                }
                                                                                <tr>
                                                                                    <td>Полное наименование государственной (муниципальной) услуги</td>
                                                                                    <td>@kcr.normativeFields.overview.servicefullName</td>
                                                                                </tr>
                                                                                @if (kcr.normativeFields.overview.arOwner != null)
                                                                                {
                                                                                    <tr>
                                                                                        <td>Полное наименование органа, разрабатывающего административны регламент предоставления государственной (муниципальной) услуги</td>
                                                                                        <td>@kcr.normativeFields.overview.arOwner.fullName</td>
                                                                                    </tr>
                                                                                }
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                    @if (kcr.normativeFields.responsibleOrganizationSelfAuthority != null && kcr.normativeFields.responsibleOrganizationSelfAuthority.Length > 0)
                                                                    {
                                                                        foreach (var selfAuthority in kcr.normativeFields.responsibleOrganizationSelfAuthority)
                                                                        {
                                                                            <div class="row justify-content-end">
                                                                                <div class="col-lg-11 accardion-milti-castoner">
                                                                                    <div class="accardion-milti-castoner-сontrol">
                                                                                        <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                            Орган, предоставляющий услугу с собственными полномочиями
                                                                                        </button>
                                                                                        <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                    </div>
                                                                                    <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                        <fieldset>
                                                                                            <div class="row justify-content-center">
                                                                                                <table class="table">
                                                                                                    <tbody>
                                                                                                        @if (selfAuthority.responsibleOrganization != null)
                                                                                                        {
                                                                                                            <tr>
                                                                                                                <td>Полное наименование органа, предоставляющего государственную (муниципальную) услугу</td>
                                                                                                                <td>@selfAuthority.responsibleOrganization.fullName</td>
                                                                                                            </tr>
                                                                                                        }
                                                                                                        @if (selfAuthority.responsibleEntities != null && selfAuthority.responsibleEntities.Length > 0)
                                                                                                        {
                                                                                                            <tr>
                                                                                                                <td>Субъект, предоставляющий услугу</td>
                                                                                                                <td>
                                                                                                                    @foreach (var item in selfAuthority.responsibleEntities)
                                                                                                                    {
                                                                                                                        <p>- @item.name</p>
                                                                                                                    }
                                                                                                                </td>
                                                                                                            </tr>
                                                                                                        }
                                                                                                    </tbody>
                                                                                                </table>
                                                                                            </div>
                                                                                        </fieldset>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        }
                                                                    }

                                                                    @if (kcr.normativeFields.responsibleOrganizationDelegateAuthority != null && kcr.normativeFields.responsibleOrganizationDelegateAuthority.Length > 0)
                                                                    {
                                                                        foreach (var delegateAuthority in kcr.normativeFields.responsibleOrganizationDelegateAuthority)
                                                                        {
                                                                            <div class="row justify-content-end">
                                                                                <div class="col-lg-11 accardion-milti-castoner">
                                                                                    <div class="accardion-milti-castoner-сontrol">
                                                                                        <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                            Орган, предоставляющий услугу с переданными полномочиями
                                                                                        </button>
                                                                                        <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                    </div>
                                                                                    <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                        <fieldset>
                                                                                            <div class="row justify-content-center">
                                                                                                <table class="table">
                                                                                                    <tbody>
                                                                                                        @if (delegateAuthority.name != null)
                                                                                                        {
                                                                                                            <tr>
                                                                                                                <td>Сфера деятельности органов, предоставляющих государственную (муниципальную) услугу</td>
                                                                                                                <td>@delegateAuthority.name</td>
                                                                                                            </tr>
                                                                                                        }
                                                                                                    </tbody>
                                                                                                </table>
                                                                                            </div>
                                                                                        </fieldset>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        }
                                                                    }

                                                                    @if (kcr.normativeFields.approved != null)
                                                                    {
                                                                        <div class="row justify-content-end">
                                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                                <div class="accardion-milti-castoner-сontrol">
                                                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                        Реквизиты акта, утверждающего регламент
                                                                                    </button>
                                                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                </div>
                                                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                    <fieldset>
                                                                                        <div class="row justify-content-center">
                                                                                            <table class="table">
                                                                                                <tbody>
                                                                                                    @if (kcr.normativeFields.approved.approvingActType != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Тип утверждающего акта</td>
                                                                                                            <td>@kcr.normativeFields.approved.approvingActType.name</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.approvingActName != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Наименование утверждающего акта</td>
                                                                                                            <td>@kcr.normativeFields.approved.approvingActName</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.approvingPersonRank != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Должность утверждающего</td>
                                                                                                            <td>@kcr.normativeFields.approved.approvingPersonRank</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.approvingPersonName != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>ФИО утверждающего</td>
                                                                                                            <td>@kcr.normativeFields.approved.approvingPersonName</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.isCancellationActSpecified)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Имеются ли акты (которыми утв. АР и изменения в него), которые необходимо признать утратившими силу?</td>
                                                                                                            <td>@(kcr.normativeFields.approved.isCancellationAct ? "да" : "нет")</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.detailsOfActCancellation != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Реквизиты акта утратившие силу</td>
                                                                                                            <td>@kcr.normativeFields.approved.detailsOfActCancellation</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.placeApprovalActTemplate != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Место утверждения акта</td>
                                                                                                            <td>@kcr.normativeFields.approved.placeApprovalActTemplate</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.npaTemplate != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Нормативные правовые акты</td>
                                                                                                            <td>@kcr.normativeFields.approved.npaTemplate</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.isSpecialOrderSpecified)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Предусмотрен ли особый порядок вступления в силу административного регламента?</td>
                                                                                                            <td>@(kcr.normativeFields.approved.isSpecialOrder ? "да" : "нет")</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.approved.numberOfDays != null)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Количество дней после публикации</td>
                                                                                                            <td>@kcr.normativeFields.approved.numberOfDays @(kcr.normativeFields.approved.numberOfDaysUnit.name ?? "")</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </fieldset>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }

                                                                    @if (kcr.normativeFields.indicators != null)
                                                                    {
                                                                        <div class="row justify-content-end">
                                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                                <div class="accardion-milti-castoner-сontrol">
                                                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                        Показатели доступности и качества
                                                                                    </button>
                                                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                </div>
                                                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                    <fieldset>
                                                                                        <div class="row justify-content-center">
                                                                                            <table class="table">
                                                                                                <tbody>
                                                                                                    @if (kcr.normativeFields.indicators.Availability != null && kcr.normativeFields.indicators.Availability.Length > 0)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Показатели доступности</td>
                                                                                                            <td>
                                                                                                                @foreach (var item in kcr.normativeFields.indicators.Availability)
                                                                                                                {
                                                                                                                    <p>- @item.name</p>
                                                                                                                }
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.indicators.Quality != null && kcr.normativeFields.indicators.Quality.Length > 0)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Показатели качества</td>
                                                                                                            <td>
                                                                                                                @foreach (var item in kcr.normativeFields.indicators.Quality)
                                                                                                                {
                                                                                                                    <p>- @item.name</p>
                                                                                                                }
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </fieldset>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }

                                                                    @if (kcr.normativeFields.roomSettings != null)
                                                                    {
                                                                        <div class="row justify-content-end">
                                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                                <div class="accardion-milti-castoner-сontrol">
                                                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                        Требования к помещениям
                                                                                    </button>
                                                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                </div>
                                                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                    <fieldset>
                                                                                        <div class="row justify-content-center">
                                                                                            <table class="table">
                                                                                                <tbody>
                                                                                                    @if (kcr.normativeFields.roomSettings.hasRoomRequirementSpecified)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Имеются ли требования к помещениям, в которых предоставляется услуга?</td>
                                                                                                            <td>@(kcr.normativeFields.roomSettings.hasRoomRequirement ? "да" : "нет")</td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                    @if (kcr.normativeFields.roomSettings.roomRequirements != null && kcr.normativeFields.roomSettings.roomRequirements.Length > 0)
                                                                                                    {
                                                                                                        <tr>
                                                                                                            <td>Требования к помещениям</td>
                                                                                                            <td>
                                                                                                                @foreach (var item in kcr.normativeFields.roomSettings.roomRequirements)
                                                                                                                {
                                                                                                                    <p>- @item.name</p>
                                                                                                                }
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                    }
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </fieldset>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }

                                                                    @if (kcr.normativeFields.associatedServices != null && kcr.normativeFields.associatedServices.Length > 0)
                                                                    {
                                                                        <div class="row justify-content-end">
                                                                            <div class="col-lg-11 accardion-milti-castoner">
                                                                                <div class="accardion-milti-castoner-сontrol">
                                                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                        Перечень услуг, которые являются необходимыми и обязательными для предоставления государственной услуги
                                                                                    </button>
                                                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                </div>
                                                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                    <fieldset>
                                                                                        <div class="row justify-content-center">
                                                                                            <table class="table">
                                                                                                <tbody>
                                                                                                    <tr>
                                                                                                        <td>Наименование необходимой и обязательной услуги</td>
                                                                                                        <td>
                                                                                                            @foreach (var item in kcr.normativeFields.associatedServices)
                                                                                                            {
                                                                                                                <p>- @item.name</p>
                                                                                                            }
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </fieldset>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }

                                                                    @if (kcr.normativeFields.monitoringCompliance != null)
                                                                    {
                                                                        <div class="row justify-content-end">
                                                                            <div class="col-lg-11 accardion-milti-castoner">
