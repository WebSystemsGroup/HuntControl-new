@section scripts
                            {
    @*<script src="~/Content/libs/footable/js/footable.min.js"></script>
        <script src="~/Content/libs/jquery.steps/js/jquery.steps.min.js"></script>
        <script src="~/Content/libs/jquery-validation/js/jquery.validate.min.js"></script>
        <script src="~/Content/libs/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
        <script src="~/Content/libs/jstree/jstree.min.339.js"></script>
        <script src="~/Content/js/ui-kit/jquery.serializejson.min.js"></script>*@
    <script>

        function showAdccardion(el) {
            let block = el.parentNode.parentNode;
            let content = block.querySelector('[data-accardion-content]');
            let indicator = el.parentNode.querySelector('.accardion-chevron');
            if (content.style.display == 'none') {
                $(content).fadeIn();
                indicator.classList.add('fa-chevron-down');
                indicator.classList.remove('fa-chevron-left');
            } else {
                indicator.classList.add('fa-chevron-left');
                indicator.classList.remove('fa-chevron-down');
                $(content).fadeOut();
            }
        }

        $(`[data-cr-content]`).hide();

        $('#nameList tr').on('click', function () {
            $('#nameList tr').removeClass('active');
            $(`[data-cr-content]`).hide();

            $(this).addClass('active');
            $(`#${$(this).find('[data-id]').data('id')}`).show();
        });
        function save() {
            let crIdArray = $('#cr [data-new] input').map(function (_, el) {
                return el.value;
            }).get();
            let serviceIdArray = $('#cr [data-new] select').map(function (_, el) {
                return el.value;
            }).get();
            $.ajax({
                type: 'post',
                url: '@Url.Action("RequestKcrGetDigitalRegulationsData", "Smev")',
                data: { crId: crIdArray, serviceId: serviceIdArray },
                success: function () {
                }
            });
        }

        function update(el) {
            $.ajax({
                type: 'post',
                url: '@Url.Action("RequestKcrGetDigitalRegulationsData", "Smev")',
                data: { crId: $('#nameList tr.active input').val() },
                beforeSend: function () {
                    el.disabled = true;
                },
                success: function () {
                }
            });
        }

        function updateServicaName(el) {
            let block = el.closest('[data-cr-content]').querySelector('[data-service]');
            let serviceId = block.querySelector('[name="serviceId"]').value;
            let serviceName = block.querySelector('[name="serviceName"]').value;
            $.ajax({
                type: 'post',
                url: '@Url.Action("UpdateServicaName", "Reference")',
                data: { serviceId, serviceName },
                success: function () {
                }
            });
        }

        function updateTariff(el) {
            let block = el.closest('[data-cr-content]').querySelector('[data-tariff]');
            let tariffId = block.querySelector('[name="tariffId"]').value;
            let countDay = block.querySelector('[name="countDay"]').value;
            $.ajax({
                type: 'post',
                url: '@Url.Action("UpdateTariff", "Reference")',
                data: { tariffId, countDay },
                beforeSend: function () {
                },
                success: function () {
                }
            });
        }

        function addNew() {
            let cloneBlock = document.querySelector('[data-clone]').cloneNode(true);
            cloneBlock.removeAttribute('data-clone');
            cloneBlock.style.removeProperty('display');
            let td = document.createElement('td');
            td.appendChild(cloneBlock);
            let tr = document.createElement('tr');
            tr.appendChild(td);
            document.querySelector('#crList').appendChild(tr);
        }

    var services = [];
    getServices();
        function getServices() {
            $.ajax({
                type: 'get',
                url: '@Url.Action("GetServices", "Reference")',
                success: function (data) {
                    services = [];
                    services.push(`<option value="">Не выбрано</option>`);
                    data.forEach(item => services.push(`<option value=${item.value}>${item.key}</option>`));
                    document.querySelector('[data-clone] select').innerHTML = services.join(' ');
                }
            });
        }
    </script>
