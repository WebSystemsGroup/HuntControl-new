                                                                                                                                        Административные процедуры в ответе
                                                                                                                                    </button>
                                                                                                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                                                                </div>
                                                                                                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                                                                    <fieldset>
                                                                                                                                        <div class="row justify-content-center">
                                                                                                                                            @foreach (var proc in answer.profilingAnswerProcedure)
                                                                                                                                            {
                                                                                                                                                <table>
                                                                                                                                                    <tbody>
                                                                                                                                                        @if (proc.admProcedureName != null)
                                                                                                                                                        {
                                                                                                                                                            <tr>
                                                                                                                                                                <td>Административная процедура</td>
                                                                                                                                                                <td>@proc.admProcedureName</td>
                                                                                                                                                            </tr>
                                                                                                                                                        }
                                                                                                                                                        @if (proc.timeFrameUnit != null)
                                                                                                                                                        {
                                                                                                                                                            <tr>
                                                                                                                                                                <td>Единица измерения для срока административной процедуры</td>
                                                                                                                                                                <td>@proc.timeFrameUnit.id</td>
                                                                                                                                                            </tr>
                                                                                                                                                        }
                                                                                                                                                    </tbody>
                                                                                                                                                </table>
                                                                                                                                            }
                                                                                                                                        </div>
                                                                                                                                    </fieldset>
                                                                                                                                </div>
                                                                                                                            </div>
                                                                                                                        </div>
                                                                                                                    }
                                                                                                                </fieldset>
                                                                                                            </div>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                }
                                                                                            </fieldset>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            }
                                                                        </fieldset>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        }
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                </fieldset>
                            </div>
                        </div>
                    </div>
                }
            </fieldset>
        </div>
    </div>
</div>