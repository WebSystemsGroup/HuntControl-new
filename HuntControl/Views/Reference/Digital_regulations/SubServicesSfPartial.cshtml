@model IEnumerable<WebAIS.Models.KcrModel.subServicesSFType>

<div class="row justify-content-end">
    <div class="col-lg-11 accardion-milti-castoner">
        <div class="accardion-milti-castoner-сontrol">
            <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                Подуслуги
            </button>
            <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
        </div>
        <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
            <fieldset>
                @foreach (var subService in Model)
                {
                    <div class="row justify-content-end">
                        <div class="col-lg-11 accardion-milti-castoner">
                            <div class="accardion-milti-castoner-сontrol">
                                <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                    Подуслуга
                                </button>
                                <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                            </div>
                            <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                <fieldset>
                                    @if (subService.overview != null)
                                    {
                                        <table class="table">
                                            <tbody>
                                                @if (subService.overview.id != null)
                                                {
                                                    <tr>
                                                        <td>Идентификатор подуслуги</td>
                                                        <td>@subService.overview.id</td>
                                                    </tr>
                                                }
                                                @if (subService.overview.shortName != null)
                                                {
                                                    <tr>
                                                        <td>Краткое наименование подуслуги</td>
                                                        <td>@subService.overview.shortName</td>
                                                    </tr>
                                                }
                                                @if (subService.overview.lifeSituations != null && subService.overview.lifeSituations.Length > 0)
                                                {
                                                    <tr>
                                                        <td>Субъект, предоставляющий услугу</td>
                                                        <td>
                                                            @foreach (var item in subService.overview.lifeSituations)
                                                            {
                                                                <p>- @item.name</p>
                                                            }
                                                        </td>
                                                    </tr>
                                                }
                                                @if (subService.overview.udertopics != null && subService.overview.udertopics.Length > 0)
                                                {
                                                    <tr>
                                                        <td>Тема</td>
                                                        <td>
                                                            @foreach (var item in subService.overview.udertopics)
                                                            {
                                                                <p>- @item.name</p>
                                                            }
                                                        </td>
                                                    </tr>
                                                }
                                                @if (subService.periodRegistration != null && subService.periodRegistration.maxPeriodOfProvidingMeasure != null)
                                                {
                                                    <tr>
                                                        <td>Максимальный срок предоставления, единица измерения</td>
                                                        <td>@subService.periodRegistration.maxPeriodOfProvidingMeasure.id</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    }

                                    @if (subService.applicantCategories != null && subService.applicantCategories.Length > 0)
                                    {
                                        <div class="row justify-content-end">
                                            <div class="col-lg-11 accardion-milti-castoner">
                                                <div class="accardion-milti-castoner-сontrol">
                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                        Категории лиц, имеющих право на получение услуги
                                                    </button>
                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                </div>
                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                    <fieldset>
                                                        @foreach (var cat in subService.applicantCategories)
                                                        {
                                                            <table class="table">
                                                                <tbody>
                                                                    @if (cat.applicant != null)
                                                                    {
                                                                        <tr>
                                                                            <td>Наименование категории заявителей</td>
                                                                            <td>@cat.applicant.id</td>
                                                                        </tr>
                                                                    }
                                                                    @if (cat.agentTypes != null && cat.agentTypes.Length > 0)
                                                                    {
                                                                        <tr>
                                                                            <td>Тип представителя</td>
                                                                            <td>
                                                                                @foreach (var item in cat.agentTypes)
                                                                                {
                                                                                    <p>- @item.id</p>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        }
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    @if (subService.channels != null)
                                    {
                                        @Html.Partial("~/Views/Reference/Digital_regulations/ChannelsSfPartial.cshtml", subService.channels)
                                    }
                                    @if (subService.preRecordSettings != null)
                                    {
                                        <div class="row justify-content-end">
                                            <div class="col-lg-11 accardion-milti-castoner">
                                                <div class="accardion-milti-castoner-сontrol">
                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                        Предварительная запись
                                                    </button>
                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                </div>
                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                    <fieldset>
                                                        <div class="row justify-content-center">
                                                            <table class="table">
                                                                <tbody>
                                                                    @if (subService.preRecordSettings.preRecordApplicationSpecified)
                                                                    {
                                                                        <tr>
                                                                            <td>Предусмотрена возможность предварительной записи для подачи заявления?</td>
                                                                            <td>@(subService.preRecordSettings.preRecordApplication ? "да" : "нет")</td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.preRecordSettings.preRecordApplicationMethods != null && subService.preRecordSettings.preRecordApplicationMethods.Length > 0)
                                                                    {
                                                                        <tr>
                                                                            <td>Способы предварительной записи для подачи заявления</td>
                                                                            <td>
                                                                                @foreach (var item in subService.preRecordSettings.preRecordApplicationMethods)
                                                                                {
                                                                                    <p>- @item.prerecordingMethod.id</p>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.preRecordSettings.preRecordResultSpecified)
                                                                    {
                                                                        <tr>
                                                                            <td>Предусмотрена возможность предварительной записи для получения результата?</td>
                                                                            <td>@(subService.preRecordSettings.preRecordResult ? "да" : "нет")</td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.preRecordSettings.preRecordResultMethods != null && subService.preRecordSettings.preRecordResultMethods.Length > 0)
                                                                    {
                                                                        <tr>
                                                                            <td>Способы предварительной записи для получения результата</td>
                                                                            <td>
                                                                                @foreach (var item in subService.preRecordSettings.preRecordResultMethods)
                                                                                {
                                                                                    <p>- @item.prerecordingMethod.id</p>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.preRecordSettings.preRecordAdministrativeProcedureSpecified)
                                                                    {
                                                                        <tr>
                                                                            <td>Предусмотрена возможность предварительной записи для выполнения административных процедур?</td>
                                                                            <td>@(subService.preRecordSettings.preRecordAdministrativeProcedure ? "да" : "нет")</td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.preRecordSettings.preRecordAdministrativeProcedures != null && subService.preRecordSettings.preRecordAdministrativeProcedures.Length > 0)
                                                                    {
                                                                        <tr>
                                                                            <td>Способы предварительной записи для выполнения административных процедур</td>
                                                                            <td>
                                                                                @foreach (var item in subService.preRecordSettings.preRecordAdministrativeProcedures)
                                                                                {
                                                                                    <p>- @item.prerecordingMethod.id</p>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    @if (subService.results != null)
                                    {
                                        <div class="row justify-content-end">
                                            <div class="col-lg-11 accardion-milti-castoner">
                                                <div class="accardion-milti-castoner-сontrol">
                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                        Результат
                                                    </button>
                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                </div>
                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                    <fieldset>
                                                        <div class="row justify-content-center">
                                                            <table class="table">
                                                                <tbody>
                                                                    @if (subService.results.resultAmount != null)
                                                                    {
                                                                        <tr>
                                                                            <td>Количество результатов</td>
                                                                            <td>@subService.results.resultAmount.id</td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                        @if (subService.results.resultsInfo != null && subService.results.resultsInfo.Length > 0 )
                                                        {
                                                            foreach (var result in subService.results.resultsInfo)
                                                            {
                                                                <div class="row justify-content-end">
                                                                    <div class="col-lg-11 accardion-milti-castoner">
                                                                        <div class="accardion-milti-castoner-сontrol">
                                                                            <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                Результат предоставления услуги
                                                                            </button>
                                                                            <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                        </div>
                                                                        <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                            <fieldset>
                                                                                <div class="row justify-content-center">
                                                                                    @if (result.resultName != null)
                                                                                    {
                                                                                        <table class="table">
                                                                                            <tbody>
                                                                                                <tr>
                                                                                                    <td>Наименование результата</td>
                                                                                                    <td>@result.resultName</td>
                                                                                                </tr>
                                                                                            </tbody>
                                                                                        </table>
                                                                                    }
                                                                                </div>
                                                                                @if (result.resultDocumentSettings != null)
                                                                                {
                                                                                    <div class="row justify-content-end">
                                                                                        <div class="col-lg-11 accardion-milti-castoner">
                                                                                            <div class="accardion-milti-castoner-сontrol">
                                                                                                <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                                    Документ, являющийся результатом
                                                                                                </button>
                                                                                                <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                            </div>
                                                                                            <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                                <fieldset>
                                                                                                    <div class="row justify-content-center">
                                                                                                        <table class="table">
                                                                                                            <thead>
                                                                                                                <tr>
                                                                                                                    <th>Наименование</th>
                                                                                                                    <th>Форма</th>
                                                                                                                </tr>
                                                                                                            </thead>
                                                                                                            <tbody>
                                                                                                                @foreach (var doc in result.resultDocumentSettings)
                                                                                                                {
                                                                                                                    <tr>
                                                                                                                        <td>@(doc.resultDocument != null ? doc.resultDocument.id : "")</td>
                                                                                                                        <td>
                                                                                                                            @if (doc.resultDocumentForms != null)
                                                                                                                            {
                                                                                                                                foreach (var item in doc.resultDocumentForms)
                                                                                                                                {
                                                                                                                                    <p>- @item.id</p>
                                                                                                                                }    
                                                                                                                            }
                                                                                                                            
                                                                                                                        </td>
                                                                                                                    </tr>
                                                                                                                }
                                                                                                            </tbody>
                                                                                                        </table>
                                                                                                    </div>
                                                                                                </fieldset>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                }
                                                                            </fieldset>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            }
                                                        }
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    @if (subService.resourceConstraint != null)
                                    {
                                        <div class="row justify-content-end">
                                            <div class="col-lg-11 accardion-milti-castoner">
                                                <div class="accardion-milti-castoner-сontrol">
                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                        Ограниченность ресурсов
                                                    </button>
                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                </div>
                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                    <fieldset>
                                                        <div class="row justify-content-center">
                                                            <table class="table">
                                                                <tbody>
                                                                    @if (subService.resourceConstraint.Constraint != null)
                                                                    {
                                                                        <tr>
                                                                            <td>Метод распределения ограниченного ресурса</td>
                                                                            <td>@subService.resourceConstraint.Constraint.id</td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.resourceConstraint.resourceConstraintObjectId != null)
                                                                    {
                                                                        <tr>
                                                                            <td>Объект, подлежащий распределению</td>
                                                                            <td>@subService.resourceConstraint.resourceConstraintObjectId.id</td>
                                                                        </tr>
                                                                    }
                                                                    @if (subService.resourceConstraint.resourceConstraintsNpa != null && subService.resourceConstraint.resourceConstraintsNpa.Length > 0)
                                                                    {
                                                                        <tr>
                                                                            <td>НПА, которым установлены ограничения ресурсов</td>
                                                                            <td>
                                                                                @foreach (var item in subService.resourceConstraint.resourceConstraintsNpa)
                                                                                {
                                                                                    <p>- @item.id</p>
                                                                                }
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    @if (subService.expertProcedure != null && subService.expertProcedure.Length > 0)
                                    {
                                        <div class="row justify-content-end">
                                            <div class="col-lg-11 accardion-milti-castoner">
                                                <div class="accardion-milti-castoner-сontrol">
                                                    <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                        Процедура оценки заявителя
                                                    </button>
                                                    <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                </div>
                                                <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                    <fieldset>
                                                        @foreach (var expertProcedure in subService.expertProcedure)
                                                        {
                                                            <div class="row justify-content-end">
                                                                <div class="col-lg-11 accardion-milti-castoner">
                                                                    <div class="accardion-milti-castoner-сontrol">
                                                                        <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                            Параметры процедуры оценки
                                                                        </button>
                                                                        <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                    </div>
                                                                    <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                        <fieldset>
                                                                            <div class="row justify-content-center">
                                                                                <table class="table">
                                                                                    <tbody>
                                                                                        @if (expertProcedure.expertProcedureType != null)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Вид процедуры оценки</td>
                                                                                                <td>@expertProcedure.expertProcedureType.id</td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedurePurpose != null && expertProcedure.expertProcedurePurpose.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Предмет процедуры оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedurePurpose)
                                                                                                    {
                                                                                                        <p>- @item.id</p>
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureObject != null && expertProcedure.expertProcedureObject.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Объект процедуры оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedureObject)
                                                                                                    {
                                                                                                        <p>- @item.id</p>
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureDurationUnit != null)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Единица измерения</td>
                                                                                                <td>@expertProcedure.expertProcedureDurationUnit.id</td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureOrganizations != null && expertProcedure.expertProcedureOrganizations.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Перечень органов власти, участвующих в процедуре оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedureOrganizations)
                                                                                                    {
                                                                                                        <p>- @item.id</p>
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureMinorOrganizations != null && expertProcedure.expertProcedureMinorOrganizations.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Перечень организаций, участвующих в процедуре оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedureMinorOrganizations)
                                                                                                    {
                                                                                                        <p>- @item.id</p>
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureResults != null && expertProcedure.expertProcedureResults.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Результаты процедуры оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedureResults)
                                                                                                    {
                                                                                                        if (item.ProcedureResult != null)
                                                                                                        {
                                                                                                            <p>- @item.ProcedureResult.id</p>
                                                                                                        }
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureNpas != null && expertProcedure.expertProcedureNpas.Length > 0)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>НПА, содержащий методику проведения процедуры оценки</td>
                                                                                                <td>
                                                                                                    @foreach (var item in expertProcedure.expertProcedureNpas)
                                                                                                    {
                                                                                                        <p>- @item.id</p>
                                                                                                    }
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                        @if (expertProcedure.expertProcedureApplicantSpecified)
                                                                                        {
                                                                                            <tr>
                                                                                                <td>Необходимо участие заявителя в процедуре оценки?</td>
                                                                                                <td>@(expertProcedure.expertProcedureApplicant ? "да" : "нет")</td>
                                                                                            </tr>
                                                                                        }
                                                                                    </tbody>
                                                                                </table>
                                                                            </div>
                                                                            @if (expertProcedure.expertProcedureTimes != null && expertProcedure.expertProcedureTimes.Length > 0)
                                                                            {
                                                                                <div class="row justify-content-end">
                                                                                    <div class="col-lg-11 accardion-milti-castoner">
                                                                                        <div class="accardion-milti-castoner-сontrol">
                                                                                            <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                                Согласования времени проведения процедуры оценки
                                                                                            </button>
                                                                                            <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                        </div>
                                                                                        <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                            <fieldset>
                                                                                                @foreach (var expertProcedureTimes in expertProcedure.expertProcedureTimes)
                                                                                                {
                                                                                                    <div class="row justify-content-end">
                                                                                                        <div class="col-lg-11 accardion-milti-castoner">
                                                                                                            <div class="accardion-milti-castoner-сontrol">
                                                                                                                <button type="button" class="btn accardion-milti-castoner-btn-show" data-accardion onclick="showAdccardion(this)">
                                                                                                                    Согласование времени проведения процедуры оценки
                                                                                                                </button>
                                                                                                                <i class="fa fa-chevron-left accardion-chevron" aria-hidden="true"></i>
                                                                                                            </div>
                                                                                                            <div class="accardion-content-milti-castoner" data-accardion-content style="display: none">
                                                                                                                <fieldset>
                                                                                                                    <div class="row justify-content-center">
                                                                                                                        <table class="table">
                                                                                                                            <tbody>
                                                                                                                                @if (expertProcedureTimes.hasexpertProcedureTimeSpecified)
                                                                                                                                {
                                                                                                                                    <tr>
                                                                                                                                        <td>Необходимо согласование времени проведения процедуры	оценки?</td>
                                                                                                                                        <td>@(expertProcedureTimes.hasexpertProcedureTime ? "да" : "нет")</td>
                                                                                                                                    </tr>
                                                                                                                                }
                                                                                                                                @if (expertProcedureTimes.expertProcedureTimeFrame != null)
                                                                                                                                {
                                                                                                                                    <tr>
                                                                                                                                        <td>Срок согласования времени проведения процедуры оценки</td>
                                                                                                                                        <td>@expertProcedureTimes.expertProcedureTimeFrame @(expertProcedureTimes.expertProcedureTimeFrameUnit != null ? expertProcedureTimes.expertProcedureTimeFrameUnit.id : "")</td>
                                                                                                                                    </tr>
                                                                                                                                }
                                                                                                                            </tbody>
                                                                                                                        </table>
                                                                                                                    </div>
                                                                                                                </fieldset>
                                                                                                            </div>
                                                                                                        </div>
