                                                                                                                                                                                        }
                                                                                                                                                                                    </td>
                                                                                                                                                                                </tr>
                                                                                                                                                                            }
                                                                                                                                                                            @if (paymentOrder.paymentAmount != null)
                                                                                                                                                                            {
                                                                                                                                                                                <tr>
                                                                                                                                                                                    <td>Сумма</td>
                                                                                                                                                                                    <td>@paymentOrder.paymentAmount</td>
                                                                                                                                                                                </tr>
                                                                                                                                                                            }
                                                                                                                                                                        </tbody>
                                                                                                                                                                    </table>
                                                                                                                                                                }
                                                                                                                                                            </div>
                                                                                                                                                        </fieldset>
                                                                                                                                                    </div>
                                                                                                                                                </div>
                                                                                                                                            </div>
                                                                                                                                        }
                                                                                                                                    </fieldset>
                                                                                                                                </div>
                                                                                                                            </div>
                                                                                                                        </div>
